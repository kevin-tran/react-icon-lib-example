    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "html-parse-stringify": "^1.0.2",
    "html-parse-stringify2": "^2.0.1",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
